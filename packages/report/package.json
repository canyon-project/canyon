{
  "name": "canyon-report",
  "version": "3.0.0-beta.2",
  "type": "module",
  "main": "./dist/canyon-report.umd.cjs",
  "module": "./dist/canyon-report.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/canyon-report.umd.cjs",
      "import": "./dist/canyon-report.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "prepublishOnly": "vite build",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx}'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "typecheck": "tsc --noEmit",
    "publish-18": "node scripts/publish-18.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@monaco-editor/react": "^4.7.0",
    "antd": "5.23.2",
    "canyon-data": "2.0.0-beta.31",
    "canyon-report-spa": "0.0.1-beta.3",
    "istanbul-lib-coverage": "*",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-highlight-words": "^0.21.0"
  },
  "devDependencies": {
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/react-highlight-words": "^0.16.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "*",
    "babel-plugin-react-compiler": "*",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "*",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.29.1",
    "typescript": "*",
    "vite": "*",
    "vite-plugin-compression2": "^0.11.0"
  }
